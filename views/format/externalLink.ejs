<% if (url != null && url != '') { %>
    <%
        var linkText = url;
        if (url.match(' ')) {
            linkText = url.slice(url.indexOf(' ') + 1);
            url = url.slice(0, url.indexOf(' '));
        }
        if (url.match('http') == null) {
            url = 'https://' + url;
        }
    %>
    <a href="<%= url %>" target="_blank"><%= linkText %></a>
<% } %>
