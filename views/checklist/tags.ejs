<%
  const meaning = {};

  meaning['cropped'] = 'Enable a note about "the images has been cropped".';

  meaning['historical'] = 'A historical event that is only relevent ' +
    'for certain people. It will appear in their profile timeline ' +
    'designated as historical but not their family members\' timelines. ' +
    'It might appear in special timelines depending on its other tags. '+
    'The list of tagged people is never visible.';

  meaning['special historical'] = 'Same as historical except that the list ' +
    'tagged people IS visible when included in special timelines.';
%>
<h1>Checklist - Tags</h1>

<h3>total: <%= Object.keys(tags).length %> tags</h3>

<h2>defined</h2>

<ul class="bullet-list">
  <% for (let tag in meaning) { %>
    <li>
      <b><%= tag %></b> - <%= tags[tag] || 0 %>
      <% if (meaning[tag]) { %>
        <br><%= meaning[tag] %>
      <% } %>
    </li>
    <% delete tags[tag]; %>
  <% } %>
</ul>

<h2>not defined</h2>

<ul class="bullet-list">
  <% for (let tag in tags) { %>
    <li>
      <b><%= tag %></b> - <%= tags[tag] %>
      <% if (meaning[tag]) { %>
        <br><%= meaning[tag] %>
      <% } %>
    </li>
  <% } %>
</ul>
