<h1>Checklist - Tags</h1>

<h3>total: <%= totalNumTags %> tags</h3>

<h2>not defined</h2>

<ul class="bullet-list">
  <% tagsUndefined.forEach(tag => { %>
    <li>
      <%- include('./_link', {tag}) %>
      - <%= tag.count %>
    </li>
  <% }); %>
</ul>

<h2>defined</h2>

<ul class="bullet-list">
  <% tagsDefined.forEach(tag => { %>
    <li>
      <%- include('./_link', {tag}) %>
      - <%= tag.count %>
      <br><%= tag.definition %>
    </li>
  <% }); %>
</ul>
