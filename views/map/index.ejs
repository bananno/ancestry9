<h2>Map</h2>

<% for (let country in pins) { %>
    <h3><%= country %></h3>
    <% for (let region1 in pins[country]) { %>
        <% for (let region2 in pins[country][region1]) { %>
            <% for (let city in pins[country][region1][region2]) { %>
                <% pins[country][region1][region2][city].forEach(pin => { %>
                    <p>
                        <%= pin %>
                    </p>
                <% });%>
            <% } %>
        <% } %>
    <% } %>
<% } %>
